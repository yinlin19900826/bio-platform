<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.biocome.platform.admin.mapper.UserCameraGroupBindMapper">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.biocome.platform.admin.entity.UserCameraGroupBind" id="userCameraGroupBindMap">
        <result property="id" column="id"/>
        <result property="username" column="username"/>
        <result property="groupIds" column="group_ids"/>
    </resultMap>

    <select id="selectBindIds" resultType="java.lang.String">
        select group_ids
        from user_camera_group_bind
        where username = #{username}
    </select>
</mapper>